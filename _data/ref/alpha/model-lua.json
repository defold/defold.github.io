                {
                    "doc": "the model",
                    "name": "url",
                    "types": [
                        "string",
                        "hash",
                        "url"
                    ]
                }
            ],
            "replaces": "",
            "returnvalues": [
                {
                    "doc": "A table containing AABB of the model. If model has no meshes - return vmath.vector3(0,0,0) for min and max fields.",
                    "name": "aabb",

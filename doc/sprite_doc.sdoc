    <span class="k">if</span> <span class="n">message</span><span class="p">.</span><span class="n">id</span> <span class="o">==</span> <span class="n">hash</span><span class="p">(</span><span class="s2">&quot;</span><span class="s">jump&quot;</span><span class="p">)</span> <span class="k">then</span>
